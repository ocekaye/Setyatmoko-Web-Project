    link(rel='stylesheet', href='css/style.css')
    // FOR IE9 below
    //if lt IE 9
      script(src='js/modernizr-2.6.2.min.js')
      script(src='js/respond.min.js')
  body.inner-page
    // START #fh5co-header
    header#fh5co-header-section(role='header')
      .container
        // START #fh5co-logo
        h1#fh5co-logo.pull-left
          a(href= baseTo) #{title}
        // START #fh5co-menu-wrap
        nav#fh5co-menu-wrap(role='navigation')
          ul#fh5co-primary-menu.sf-menu
            li
              a(href= baseTo) Home
            li.active
              a.fh5co-sub-ddown(href='#') Dropdown
              ul.fh5co-sub-menu
                li.active
                  a(href='left-sidebar.html') Left Sidebar
                li
                  a(href='right-sidebar.html') Right Sidebar
                li
                  a(href='#') HTML5
                li
                  a.fh5co-sub-ddown(href='#') JavaScript
                  ul.fh5co-sub-menu
                    li
                      a(href='#') jQuery
                    li
                      a(href='#') Zipto
                    li
                      a(href='#') Node.js
                    li
                      a(href='#') AngularJS
                li
                  a(href='#') CSS3
            li
              a(href='elements.html') Elements
            li
              a(href='contact.html') Contact
    // START #fh5co-hero
    aside#fh5co-hero(style='background-image: url(images/hero.jpg);')
      .container
        .row
          .col-md-8.col-md-offset-2
            .fh5co-hero-wrap
              .fh5co-hero-intro
                h2
                  | Inner Page
                  span
                h1 #{title}
    #fh5co-main
      section
        .container
          .row
            #fh5co-content.col-md-8.col-md-offset-1.col-md-push-3
              .row
                each val in datas.new
                  .col-md-12
                    if(val.picture)
                      h2= val.title
                      .col-md-4.col-sm-4
                        a(href = '#')
                          img.img-responsive(src= val.picture, alt='Image')
                      .col-md-8.col-sm-8
                        p= val.content_description
                        p
                          a.btn.btn-primary.btn-sm(href= to+val.uri) Read More
                    else
                        h2= val.title
                        p= val.content_description
                        p
                          a.btn.btn-primary.btn-sm(href= to+val.uri) Read More
                  .fh5co-spacer.fh5co-spacer-xs
                .fh5co-tab.text-center
                  ul.resp-tabs-list.hor_1
                    each p in datas.pagination.data
                      if(p.url == "#")
                        li
                          if(p.text == 'prev')
                            span.glyphicon.glyphicon-chevron-left
                          else if(p.text == 'next')
                            span.glyphicon.glyphicon-chevron-right
                          else
                            if(datas.pagination.page == p.text)
                              .resp-tab-active= p.text
                            else
                              = p.text
                      else
                        a(href=p.url)
                          li
                            if(p.text == 'prev')
                              span.glyphicon.glyphicon-chevron-left
                            else if(p.text == 'next')
                              span.glyphicon.glyphicon-chevron-right
                            else
                              if(datas.pagination.page == p.text)
                                .resp-tab-active= p.text
                              else
                                = p.text
              .fh5co-spacer.fh5co-spacer-sm
            #fh5co-sidebar.col-md-3.col-md-pull-9
              .fh5co-side-section.fh5co-nav-links
                h2.fh5co-uppercase-heading-sm Categorys
                ul
                  each val in datas.categorys
                    li
                      a(href='#')= val.name

              .fh5co-spacer.fh5co-spacer-sm
              .fh5co-side-section
                h2.fh5co-uppercase-heading-sm Sweet Little <p>
                p
                  | Magnam consectetur quia illo rerum aliquam doloremque omnis assumenda quasi enim ex totam provident dignissimos numquam laudantium
                p
                  a(href='#') Go! click me
              .fh5co-spacer.fh5co-spacer-sm
              .fh5co-side-section
                h2.fh5co-uppercase-heading-sm Image Magnific Popup
                .fh5co-spacer.fh5co-spacer-xxs
                .row
                  .col-md-12
                    p
                      a.image-popup(href='images/project_5.jpg')
                        img.img-responsive(src='images/project_5.jpg', alt='Image')
                  .col-md-6
                    p
                      a.image-popup(href='images/project_6.jpg')
                        img.img-responsive(src='images/project_6.jpg', alt='Image')
                  .col-md-6
                    a.image-popup(href='images/project_7.jpg')
                      img.img-responsive(src='images/project_7.jpg', alt='Image')
                    p
          .fh5co-spacer.fh5co-spacer-md
      footer#fh5co-footer
        .container
          ul.fh5co-social-icons
            li
              a(href='#')
                i.ti-twitter-alt
            li
              a(href='#')
                i.ti-facebook
            li
              a(href='#')
                i.ti-github
            li
              a(href='#')
                i.ti-linkedin
          p.text-muted.fh5co-no-margin-bottom.text-center
            small
              | Â© 2015 
              a(href='#') Display
              | . All rights reserved. Crafted with love 
              em by
              a(href='http://freehtml5.co', target='_blank') FREEHTML5.co
              br
              |  Images by 
              a(href='http://unsplash.com/', target='_blank') Unsplash
